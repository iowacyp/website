{
  "name": "iowacyp-site",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "ELEVENTY_ENV=dev concurrently \"npm:watch:css\" \"npm:serve\"",
    "build": "eleventy && npm run build:css",
    "serve": "eleventy --serve --quiet",
    "watch:css": "postcss src/assets/css/tailwind.css -o dist/assets/css/site.css --watch",
    "build:css": "postcss src/assets/css/tailwind.css -o dist/assets/css/site.css"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "postcss": "^8.4.35",
    "postcss-cli": "^11.0.0",
    "tailwindcss": "^3.4.10"
  }
}